    var x = this.$router;\r\n            var y = this.user\r\n            x.push({name: \"ThongTinNhanVien\",params: {data: y}})\r\n        },\r\n        DoiMatKhau(){\r\n            var x = this.$router;\r\n            var y = this.user\r\n            x.push({name: \"DoiMatKhau\",params: {data: y}})\r\n        },\r\n        async GuiDon(){\r\n          const nv = await axios.get(\"https://localhost:5001/api/NhanViens/Get/\"+this.user.username);\r\n          this.Don.MaNV = nv.data.MaNV;\r\n          await axios.post(\"https://localhost:5001/api/DonXins\",this.Don);\r\n          alert(\"Gửi Thành Công\");\r\n        }\r\n    }\r\n  }\r\n</script>"]}]}