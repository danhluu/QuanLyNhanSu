ios from 'axios'\r\nexport default {\r\n    props:['dialogUpdate','HoSoNhanVien'],\r\n    data(){\r\n        return{\r\n            loading: false,\r\n            items: [ \r\n\r\n            ],\r\n            //   NhanVien:{},\r\n            dialog: this.dialogUpdate,\r\n            usernameRules: [\r\n                v => !!v || 'Tên Tài Khoản ít nhất 2 ký tự',\r\n                v => (v && v.length >= 2) || 'Tên ít nhất 2 ký tự',\r\n            ],\r\n            passwordRules:[\r\n                v => !!v || 'Mật Khẩu ít nhất 6 ký tự'\r\n            ],\r\n            EmptyRules:[\r\n                v=>!!v||'Không được bỏ trống'\r\n            ],\r\n        }\r\n    },\r\n    methods:{\r\n        \r\n        btnCancelOnClick(){\r\n            this.$emit('closePopup',false);\r\n        },\r\n        async saveProduct(){\r\n            if(this.$refs.formValidate.validate()){\r\n                console.log(this.HoSoNhanVien)\r\n                if(this.HoSoNhanVien.MaHSNV==undefined){\r\n                    this.loading=true;\r\n                    await axios.post(\"https://localhost:5001/api/HoSoNhanViens\",this.HoSoNhanVien);\r\n                    console.log(this.HoSoNhanVien)\r\n                    this.HoSoNhanVien = {};\r\n                    this.$emit('closePopup',false);\r\n                }\r\n                else{\r\n                    this.loading=true;\r\n                    console.log(this.HoSoNhanVien)\r\n                    await axios.put(\"https://localhost:5001/api/HoSoNhanViens\",this.HoSoNhanVien);\r\n                    this.HoSoNhanVien = {};\r\n                    this.$emit('closePopup',false);\r\n                }\r\n                this.$emit(\"loadDL\");\r\n                \r\n                \r\n            }\r\n            this.loading=false;\r\n        },\r\n        \r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.btn-close{\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n}\r\n.dateField{\r\n    margin-top: 20px;\r\n  